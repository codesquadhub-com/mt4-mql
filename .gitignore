# do not ignore the following files
!.gitattributes

# ignore Eclipse directories and files
/.externalToolBuilders/
/.settings/
/.buildpath
/.project*

# ignore the following directories and files
/etc/windows/appdata/*
mqlcache.dat
*.ex4
*.ex5

# ignore generated and log files in legacy MQL4 structure
/mql4/experts/config/
/mql4/experts/config
/mql4/experts/files/
/mql4/experts/files
/mql4/experts/logs/
/mql4/experts/logs
/mql4/experts/presets/
/mql4/experts/presets
/mql4/experts/templates/
/mql4/experts/templates

# ignore linked MQL4 sources in legacy MQL4 structure
/mql4/experts/include/
/mql4/experts/include
/mql4/experts/indicators/
/mql4/experts/indicators
/mql4/experts/libraries/
/mql4/experts/libraries
/mql4/experts/scripts/
/mql4/experts/scripts

# ignore DLL build artefacts
/mql4/libraries/*.Debug.dll
/mql4/libraries/*.Release.dll

# ignore generated and log files in MQL4/5 structures
/mql*/Files/
/mql*/files/
/mql*/files
/mql*/Logs/
/mql*/logs/
/mql*/logs
/mql*/Presets/
/mql*/presets/
/mql*/presets

/mql*/Images/
/mql*/Images
/mql*/Projects/
/mql*/Projects
/mql*/Shared Projects/
/mql*/Shared Projects

# ignore everything generated by the tester
/tester/

# do not ignore the following files
!.gitattributes

# ignore Eclipse directories and files
/.externalToolBuilders/
/.settings/
/.buildpath
/.project*

# ignore the following directories and files
/tester/
mqlcache.dat
*.ex4

# ignore generated and log files in MQL4/5 structures
/mql*/Files/
/mql*/files/
/mql*/files

/mql*/Logs/
/mql*/logs/
/mql*/logs

/mql*/Presets/
/mql*/presets/
/mql*/presets

/mql*/Images/
/mql*/Images
/mql*/Projects/
/mql*/Projects

# ignore generated and log files in legacy MQL4 structure
/mql4/experts/config/
/mql4/experts/files/
/mql4/experts/files
/mql4/experts/logs/
/mql4/experts/logs
/mql4/experts/presets/
/mql4/experts/presets
/mql4/experts/templates/

# ignore linked MQL4 sources in legacy MQL4 structure
/mql4/experts/include/
/mql4/experts/include
/mql4/experts/indicators/
/mql4/experts/indicators
/mql4/experts/libraries/
/mql4/experts/libraries
/mql4/experts/scripts/
/mql4/experts/scripts

# ignore linked MQL5 programs in current MT4 structure
/mql4/experts/- MQL5
/mql4/indicators/- MQL5
/mql4/scripts/- MQL5
